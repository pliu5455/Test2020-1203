 package demo;

public class MyDate {
  private int year;
  private int month;
  private int day;
  
  // year get method
  public int getYear() {
	  return this.year;
}
  // year set method
  public void setYear(int year) {
	  this.year = year;
	
}
public int getMonth() {
	return month;
}
public void setMonth(int month) {
	this.month = month;
}
public int getDay() {
	return day;
}
public void setDay(int day) {
	this.day = day;
}
public void printDate() {
	System.out.println(year + "-" + month + "-" + day);
	
}
}
